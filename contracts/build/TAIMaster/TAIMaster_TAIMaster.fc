(slice, slice, int, int, cell, int) $TAIMaster$_contract_load() impure inline {
    slice $sc = get_data().begin_parse();
    int $loaded = $sc~load_int(1);
    if ($loaded) {
        return $sc~$TAIMaster$_load();
    }
    else {
        (slice $owner, slice $vesting) = $sc~$TAIMaster$init$_load();
        $sc.end_parse();
        return $TAIMaster$_contract_init($owner, $vesting);
    }
}

() $TAIMaster$_contract_store((slice, slice, int, int, cell, int) v) impure inline {
    builder b = begin_cell();
    b = b.store_int(true, 1);
    b = $TAIMaster$_store(b, v);
    set_data(b.end_cell());
}

