(slice, slice, int, int, cell, int) $TAIMaster$_contract_init(slice $owner, slice $vesting) impure inline {
    var (($self'owner, $self'vesting, $self'totalSupply, $self'lockedSupply, $self'balances, $self'lockedMoved)) = (null(), null(), null(), null(), null(), null());
    $self'owner = $owner;
    $self'vesting = $vesting;
    $self'totalSupply = 100000000000;
    $self'lockedSupply = 90000000000;
    $self'lockedMoved = false;
    $self'balances = null();
    $self'balances~__tact_dict_set_slice_int(267, $owner, 100000000000, 257);
    return ($self'owner, $self'vesting, $self'totalSupply, $self'lockedSupply, $self'balances, $self'lockedMoved);
}

((slice, slice, int, int, cell, int), (int, int, int, int)) $TAIMaster$_fun_summary((slice, slice, int, int, cell, int) $self) impure inline_ref {
    var (($self'owner, $self'vesting, $self'totalSupply, $self'lockedSupply, $self'balances, $self'lockedMoved)) = $self;
    int $circulating = ($self'totalSupply - $self'lockedSupply);
    var $fresh$ret_0 = $SupplySummary$_constructor_total_locked_circulating_lockedTransferred($self'totalSupply, $self'lockedSupply, $circulating, $self'lockedMoved);
    return (($self'owner, $self'vesting, $self'totalSupply, $self'lockedSupply, $self'balances, $self'lockedMoved), $fresh$ret_0);
}

((slice, slice, int, int, cell, int), int) $TAIMaster$_fun_balanceOfInternal((slice, slice, int, int, cell, int) $self, slice $target) impure inline_ref {
    var (($self'owner, $self'vesting, $self'totalSupply, $self'lockedSupply, $self'balances, $self'lockedMoved)) = $self;
    int $saved = __tact_dict_get_slice_int($self'balances, 267, $target, 257);
    if (null?($saved)) {
        var $fresh$ret_2 = 0;
        return (($self'owner, $self'vesting, $self'totalSupply, $self'lockedSupply, $self'balances, $self'lockedMoved), $fresh$ret_2);
    }
    var $fresh$ret_3 = __tact_not_null($saved);
    return (($self'owner, $self'vesting, $self'totalSupply, $self'lockedSupply, $self'balances, $self'lockedMoved), $fresh$ret_3);
}

((slice, slice, int, int, cell, int), int) $TAIMaster$_fun_balanceOf((slice, slice, int, int, cell, int) $self, slice $target) impure inline_ref {
    var (($self'owner, $self'vesting, $self'totalSupply, $self'lockedSupply, $self'balances, $self'lockedMoved)) = $self;
    var $fresh$ret_1 = ($self'owner, $self'vesting, $self'totalSupply, $self'lockedSupply, $self'balances, $self'lockedMoved)~$TAIMaster$_fun_balanceOfInternal($target);
