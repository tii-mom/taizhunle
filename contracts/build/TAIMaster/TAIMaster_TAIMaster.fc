    var build_1 = begin_cell();
    build_1 = build_1.store_int(v'lockedSupply, 257);
    build_1 = build_1.store_dict(v'balances);
    build_1 = build_1.store_int(v'lockedMoved, 1);
    build_0 = store_builder_ref(build_0, build_1);
    return build_0;
