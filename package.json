{
  "name": "taizhunle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --host 0.0.0.0 --port 5173",
    "dev:server": "tsx watch --env-file=.env src/server/main.ts",
    "build": "vite build && tsc -p tsconfig.server.json --skipLibCheck --noEmitOnError false",
    "build:full": "tsc -b && vite build && tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json --skipLibCheck --noEmitOnError false",
    "start": "node dist/server/main.js",
    "lint": "eslint .",
    "test": "vitest run",
    "test:e2e": "vitest run tests/e2e.marketFlow.test.ts",
    "test:insight": "vitest run tests/marketInsightsService.test.ts",
    "test:db": "vitest run tests/marketServiceCreation.test.ts tests/daoCreation.test.ts",
    "preview": "vite preview",
    "deploy": "npm run build && vercel --prod",
    "db:reset": "npx supabase db reset --local",
    "db:push": "npx supabase db push",
    "setup": "node scripts/setup-env.cjs",
    "env:check": "node scripts/check-env.cjs",
    "env:validate": "node scripts/check-env.cjs",
    "postinstall": "npm run env:check || echo '⚠️  请运行 npm run setup 配置环境变量'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
